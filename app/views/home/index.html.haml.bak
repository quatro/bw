    / Content Wrapper. Contains page content
    .content-wrapper
      / Content Header (Page header)
      .content-header
        .container-fluid
          .row.mb-2
            .col-sm-6
              %h1.m-0.text-dark Dashboard
            / /.col
            .col-sm-6
              %ol.breadcrumb.float-sm-right
                %li.breadcrumb-item
                  %a{:href => "#"} Home
                %li.breadcrumb-item.active Dashboard v1
            / /.col
          / /.row
        / /.container-fluid
      / /.content-header
      / Main content
      %section.content
        .container-fluid
          / Small boxes (Stat box)
          .row
            .col-lg-3.col-6
              / small box
              .small-box.bg-info
                .inner
                  %h3 150
                  %p New Orders
                .icon
                  %i.ion.ion-bag
                %a.small-box-footer{:href => "#"}
                  More info
                  %i.fas.fa-arrow-circle-right
            / ./col
            .col-lg-3.col-6
              / small box
              .small-box.bg-success
                .inner
                  %h3
                    53
                    %sup{:style => "font-size: 20px"} %
                  %p Bounce Rate
                .icon
                  %i.ion.ion-stats-bars
                %a.small-box-footer{:href => "#"}
                  More info
                  %i.fas.fa-arrow-circle-right
            / ./col
            .col-lg-3.col-6
              / small box
              .small-box.bg-warning
                .inner
                  %h3 44
                  %p User Registrations
                .icon
                  %i.ion.ion-person-add
                %a.small-box-footer{:href => "#"}
                  More info
                  %i.fas.fa-arrow-circle-right
            / ./col
            .col-lg-3.col-6
              / small box
              .small-box.bg-danger
                .inner
                  %h3 65
                  %p Unique Visitors
                .icon
                  %i.ion.ion-pie-graph
                %a.small-box-footer{:href => "#"}
                  More info
                  %i.fas.fa-arrow-circle-right
            / ./col
          / /.row
          / Main row
          .row
            / Left col
            %section.col-lg-7.connectedSortable
              / Custom tabs (Charts with tabs)
              .card
                .card-header
                  %h3.card-title
                    %i.fas.fa-chart-pie.mr-1
                    Sales
                  .card-tools
                    %ul.nav.nav-pills.ml-auto
                      %li.nav-item
                        %a.nav-link.active{"data-toggle" => "tab", :href => "#revenue-chart"} Area
                      %li.nav-item
                        %a.nav-link{"data-toggle" => "tab", :href => "#sales-chart"} Donut
                / /.card-header
                .card-body
                  .tab-content.p-0
                    / Morris chart - Sales
                    #revenue-chart.chart.tab-pane.active{:style => "position: relative; height: 300px;"}
                      %canvas#revenue-chart-canvas{:height => "300", :style => "height: 300px;"}
                    #sales-chart.chart.tab-pane{:style => "position: relative; height: 300px;"}
                      %canvas#sales-chart-canvas{:height => "300", :style => "height: 300px;"}
                / /.card-body
              / /.card
              / DIRECT CHAT
              .card.direct-chat.direct-chat-primary
                .card-header
                  %h3.card-title Direct Chat
                  .card-tools
                    %span.badge.badge-primary{"data-toggle" => "tooltip", :title => "3 New Messages"} 3
                    %button.btn.btn-tool{"data-card-widget" => "collapse", :type => "button"}
                      %i.fas.fa-minus
                    %button.btn.btn-tool{"data-toggle" => "tooltip", "data-widget" => "chat-pane-toggle", :title => "Contacts", :type => "button"}
                      %i.fas.fa-comments
                    %button.btn.btn-tool{"data-card-widget" => "remove", :type => "button"}
                      %i.fas.fa-times
                / /.card-header
                .card-body
                  / Conversations are loaded here
                  .direct-chat-messages
                    / Message. Default to the left
                    .direct-chat-msg
                      .direct-chat-infos.clearfix
                        %span.direct-chat-name.float-left Alexander Pierce
                        %span.direct-chat-timestamp.float-right 23 Jan 2:00 pm
                      / /.direct-chat-infos
                      -#%img.direct-chat-img{:alt => "message user image", :src => "dist/img/user1-128x128.jpg"}/
                      / /.direct-chat-img
                      .direct-chat-text
                        Is this template really for free? That's unbelievable!
                      / /.direct-chat-text
                    / /.direct-chat-msg
                    / Message to the right
                    .direct-chat-msg.right
                      .direct-chat-infos.clearfix
                        %span.direct-chat-name.float-right Sarah Bullock
                        %span.direct-chat-timestamp.float-left 23 Jan 2:05 pm
                      / /.direct-chat-infos
                      -#%img.direct-chat-img{:alt => "message user image", :src => "dist/img/user3-128x128.jpg"}/
                      / /.direct-chat-img
                      .direct-chat-text
                        You better believe it!
                      / /.direct-chat-text
                    / /.direct-chat-msg
                    / Message. Default to the left
                    .direct-chat-msg
                      .direct-chat-infos.clearfix
                        %span.direct-chat-name.float-left Alexander Pierce
